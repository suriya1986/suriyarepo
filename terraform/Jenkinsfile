pipeline {
    agent any
    stages {
        stage ('Create S3') {
            steps {
                withCredentials([usernamePassword(credentialsId: 'AWSCredentials', credentialsType:'jenkins')]) {
                   awsCodeBuild(projectName: 'project', region: 'us-west-2', sourceControlType: 'jenkins',buildSpecFile:'build/buildterraform.yml')
                }
            }
        }
    }
}
